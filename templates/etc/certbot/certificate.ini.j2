{{ ansible_managed | comment }}

# Target DNS server
dns_rfc2136_server = {{ item.dns_server }}
# Target DNS port
dns_rfc2136_port = {{ item.dns_port }}
# TSIG key name
dns_rfc2136_name = {{ item.rfc2136_key.name }}
# TSIG key secret
dns_rfc2136_secret = {{ item.rfc2136_key.secret }}
# TSIG key algorithm
dns_rfc2136_algorithm = {{ item.rfc2136_key.algorithm | default('HMAC-SHA512')
}}
